  "scripts": {
    "dev": "nx run-many --target=serve --parallel --configuration=development",
    "dev:fe": "nx run fe:serve:development",
    "dev:api": "nx run api:serve:development",
    "format": "nx format:write",
    "lint": "nx run-many --target=lint --parallel",
    "ts-check": "nx run-many --target=ts-check --parallel --skip-nx-cache",
    "nx-reset": "nx reset",
    "nx-repair": "nx repair",
    "db:generate": "dotenv -e ./apps/api/.env prisma generate",
    "db:migrate": "dotenv -e ./apps/api/.env prisma migrate dev",
    "db:deploy": "dotenv -e ./apps/api/.env prisma migrate deploy",
    "db:studio": "dotenv -e ./apps/api/.env prisma studio",
    "db:reset": "dotenv -e ./apps/api/.env prisma migrate reset",
    "db:seed": "dotenv -e ./apps/api/.env prisma db seed",
    "postinstall": "npm run db:generate"
  },
  "private": true,
