{
  "name": "@ed-demo/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "nx run-many --target=serve --parallel --configuration=development",
    "dev:fe": "nx run fe:serve:development",
    "dev:api": "nx run api:serve:development",
    "format": "nx format:write",
    "lint": "nx run-many --target=lint --parallel",
    "check-types": "nx run-many --target=check-types --parallel --skip-nx-cache",
    "nx-reset": "nx reset",
    "nx-repair": "nx repair",
    "db:generate": "dotenv -e ./apps/api/.env prisma generate",
    "db:migrate": "dotenv -e ./apps/api/.env prisma migrate dev",
    "db:deploy": "dotenv -e ./apps/api/.env prisma migrate deploy",
    "db:studio": "dotenv -e ./apps/api/.env prisma studio",
    "db:reset": "dotenv -e ./apps/api/.env prisma migrate reset",
    "db:seed": "dotenv -e ./apps/api/.env prisma db seed",
    "postinstall": "npm run db:generate"
  },
  "private": true,
  "devDependencies": {
    "@nestjs/schematics": "^10.1.2",
    "@nestjs/testing": "^10.3.10",
    "@nx/eslint": "19.4.0",
    "@nx/eslint-plugin": "19.4.0",
    "@nx/js": "19.4.0",
    "@nx/nest": "^19.4.0",
    "@nx/node": "19.4.0",
    "@nx/react": "^19.4.0",
    "@nx/vite": "19.4.0",
    "@nx/web": "19.4.0",
    "@nx/webpack": "19.4.0",
    "@nx/workspace": "19.4.0",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "^0.3.14",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "^0.5.11",
    "@types/morgan": "^1.9.9",
    "@types/node": "18.16.9",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "@vitest/ui": "^1.3.1",
    "eslint": "~8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "jsdom": "~22.1.0",
    "nx": "19.4.0",
    "prettier": "^2.8.8",
    "prisma": "^5.16.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.2",
    "vite": "^5.3.3",
    "vitest": "^1.3.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.10",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.10",
    "@nestjs/platform-express": "^10.3.10",
    "@prisma/client": "^5.16.1",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "morgan": "^1.10.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.0",
    "tslib": "^2.3.0"
  }
